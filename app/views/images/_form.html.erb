<%= simple_form_for(@image) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :ark %>
    <%= f.input :bibid %>
    <%= f.input :title %>
    <%= f.input :publisher %>
    <%= f.input :author %>
    <%= f.input :year %>
    <%= f.input :description %>
    <%= f.input :add_date %>
    <%= f.input :upd_date %>
    <%= f.input :copyright %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
