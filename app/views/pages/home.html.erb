<div class="bs-example" data-example-id="thumbnails-with-custom-content">
  <div class="row">
    <% if cannot? :access, :rails_admin %>
      <div class="col-sm-6 col-md-2">
      </div>
    <% end %>

    <%= link_to controller: :images, action: :index do %>
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <%= image_tag "scan.jpg", alt: "100%x200", style: "height: 200px; width: 100%; display: block;", data: { src: "holder.js/100%x200", holder_rendered: "true" } %>
          <div class="caption">
            <h3 style="color: #777;">Scanned Map Images</h3>
          </div>
        </div>
      </div>
    <% end %>
    <%= link_to controller: :datasets, action: :index do %>
      <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
          <%= image_tag "dataset.png", alt: "100%x200", style: "height: 200px; width: 100%; display: block;", data: { src: "holder.js/100%x200", holder_rendered: "true" } %>
          <div class="caption">
            <h3 style="color: #777;">GIS Datasets</h3>
          </div>
        </div>
      </div>
    <% end %>
    <% if can? :access, :rails_admin %>
      <%= link_to 'admin/user' do %>
        <div class="col-sm-6 col-md-4">
          <div class="thumbnail">
            <%= image_tag "users.png", alt: "100%x200", style: "height: 200px; width: 100%; display: block;", data: { src: "holder.js/100%x200", holder_rendered: "true" } %>
            <div class="caption">
              <h3 style="color: #777;">Users</h3>
            </div>
          </div>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
